{
  "name": "electron-ts-vue",
  "version": "0.1.0",
  "description": "Electron-Typescript-Vue Starter.",
  "author": "archco",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "npm run compile:prod",
    "build": "electron-builder",
    "build:pack": "npm run build -- --dir",
    "dev": "cross-env NODE_ENV=development node task/dev-runner.js",
    "compile": "npm run compile:main && npm run compile:renderer",
    "compile:main": "webpack --config ./task/webpack.main.config.js --hide-modules",
    "compile:renderer": "webpack --config ./task/webpack.renderer.config.js --hide-modules",
    "precompile:prod": "node task/clear-dist.js",
    "compile:prod": "cross-env NODE_ENV=production npm run compile",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest test/feature",
    "test:unit": "jest test/unit"
  },
  "keywords": [],
  "build": {
    "productName": "electron-ts-vue",
    "appId": "org.archco.electron-ts-vue",
    "directories": {
      "buildResources": "resources/**/*",
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon-256.png"
    },
    "win": {
      "icon": "resources/icons/icon-256.png"
    },
    "linux": {
      "icon": "resources/icons/icon-256.png"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^24.9.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.1",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^9.0.0",
    "style-loader": "^1.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-router": "^3.1.5",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "vue"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "chrome >= 78"
          }
        }
      ]
    ]
  }
}
