{
  "name": "electron-ts-vue",
  "version": "0.1.0",
  "description": "Electron-Typescript-Vue Starter.",
  "main": "dist/main.js",
  "scripts": {
    "predev": "npm run compile",
    "dev": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"npm run compile:renderer -- --watch\" \"electron .\"",
    "compile": "npm run compile:main && npm run compile:renderer",
    "compile:main": "tsc",
    "compile:renderer": "webpack --config ./task/webpack.config.js --hide-modules",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest test/feature",
    "test:unit": "jest test/unit"
  },
  "keywords": [],
  "author": "archco",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@vue/test-utils": "^1.0.0-beta.13",
    "babel-jest": "^23.0.0-alpha.4",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jest": "^22.4.3",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "spectron": "^3.8.0",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.1",
    "vue": "^2.5.16",
    "vue-jest": "^2.4.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "vue"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "chrome >= 59"
          }
        }
      ]
    ]
  }
}
